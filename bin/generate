#!/usr/bin/env zsh

cd ~/src/ext/markdown-resume

./bin/md2resume pdf --template swissen ~/src/just3ws/resume/Résumé-for-Mike-Hall-Senior-Software-Developer-Ruby.md ~/src/just3ws/resume
./bin/md2resume html --template swissen --refresh yes ~/src/just3ws/resume/Résumé-for-Mike-Hall-Senior-Software-Developer-Ruby.md ~/src/just3ws/resume

cd -
